{
    "name": "turbo/turbo-erp-api",
    "description": "API for TurboERP",
    "type": "project",
    "authors": [
        {
            "name": "Peeratchai Petpadriew",
            "email": "p.petpadriew@gmail.com"
        }
    ],
    "require": {
        "fideloper/proxy": "~3.3",
        "laravel/framework": "^5.5",
        "laravel/tinker": "~1.0"
    },
    "require-dev": {
        "filp/whoops": "~2.0",
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "~1.0",
        "phpunit/phpunit": "~6.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "Turbo\\ERP\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Turbo\\ERP\\Tests\\": "tests/"
        }
    }
}
